generator client {
  provider = "prisma-client-js"
  output   = "../src/generated/prisma"
}

datasource db {
  provider = "postgresql"
}

model Contact {
  id          String   @id @default(uuid())
  name        String
  email       String   @unique
  companyName String?
  phone       String
  service     String
  amount      String
  message     String
  createdAt   DateTime @default(now())
  updatedAt   DateTime @updatedAt
}
